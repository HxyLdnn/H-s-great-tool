#include<iostream>
using namespace std;
int x, y;
void exgcd(int a, int b){ // void exgcd(int a, int b, int &x, int &y)
    if(b == 0){
        x = 1, y = 0;
        return ;
    }
    exgcd(b, a % b, y, x);
    y = y - a / b * x;
}

int main(){
    int n, a, b;
    cin >> n;
    while(n -- ){
        cin >> a >> b;
        exgcd(a, b, x, y);
        cout << x << ' ' << y << endl;
    }
}
